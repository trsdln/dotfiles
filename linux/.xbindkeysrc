# Increase volume
"change-volume.sh '5%+'"
   XF86AudioRaiseVolume

# Decrease volume
"change-volume.sh '5%-'"
   XF86AudioLowerVolume

# Mute volume
"change-volume.sh toggle"
   XF86AudioMute

"toggle-hdmi.sh"
   XF86Display

# Increase backlight
"xbacklight -inc 10"
   XF86MonBrightnessUp

# Decrease backlight
"xbacklight -dec 10"
   XF86MonBrightnessDown
