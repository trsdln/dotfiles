# Increase volume
"change-volume.sh '5%+'"
   XF86AudioRaiseVolume

# Decrease volume
"change-volume.sh '5%-'"
   XF86AudioLowerVolume

# Mute volume
"change-volume.sh toggle"
   XF86AudioMute

"toggle-hdmi.sh"
   XF86Display

"power-manager.sh"
   XF86Favorites

"toggle-wifi.sh"
   XF86WLAN

# Increase backlight
"change-brightness.sh '-inc 10'"
   XF86MonBrightnessUp

# Decrease backlight
"change-brightness.sh '-dec 10'"
   XF86MonBrightnessDown
